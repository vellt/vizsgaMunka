            <navButton:Product x:Name="btnTermekek" Text="Termékek" PreviewMouseUp="btnTermekek_MouseDown" />
            <navButton:Car x:Name="btnAtadas" Text="Raktárközi átadás" PreviewMouseUp="btnAtadas_MouseDown" />
