                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="40"/>
                                </Grid.RowDefinitions>
                                <!--table header-->
                                <header:Raktar Grid.Row="0"/>
                                <!--table body-->
                                <ScrollViewer Grid.Row="1" HorizontalScrollBarVisibility="Disabled">
                                    <StackPanel x:Name="spRaktarTabla" Orientation="Vertical"/>
                                </ScrollViewer>
                                <!--buttons-->
                                <StackPanel x:Name="spRaktarButtons" Grid.Row="3" Background="#E0E0E0" Orientation="Horizontal">
                                    <Grid Width="40" Height="40" Cursor="Hand" ToolTip="Raktár hozzáadása" PreviewMouseUp="RaktarakBtn" Background="#E0E0E0" >
                                        <icon:Add1 Fill="#707070"/>
                                    </Grid>
                                    <Grid Width="40" Height="40" Cursor="Hand" Tag="Secret" ToolTip="Raktár módosítása" Visibility="Collapsed" PreviewMouseUp="RaktarakBtn" Background="#E0E0E0"  >
                                        <icon:Edit Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" Tag="Secret" ToolTip="Raktár törlése" Visibility="Collapsed" PreviewMouseUp="RaktarakBtn" Background="#E0E0E0" >
                                        <icon:Bin Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" ToolTip="Szinkronizálás" PreviewMouseUp="RaktarakBtn" Background="#E0E0E0" >
                                        <icon:Synch Fill="#707070"/>
                                    </Grid>
                                </StackPanel>
                            </Grid>

                            <Grid x:Name="raktarSS1" Grid.Row="1" Visibility="Collapsed" Tag="Raktár SubScreen">
                                <Grid.Background>
                                    <SolidColorBrush Color="Black" Opacity="0.5"/>
                                </Grid.Background>
                                <Grid Height="300" Width="540">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Width="540" Grid.Row="0" Background="#707070" HorizontalAlignment="Right">
                                        <Label x:Name="raktarSS1Fejlec" HorizontalAlignment="Left" Content="Fejléc" Foreground="White" FontSize="14" FontFamily="Segoe UI" Margin="5 0"/>
                                        <!--button-->
                                        <Grid Width="30" Height="30" ToolTip="Vissza" Cursor="Hand" HorizontalAlignment="Right" PreviewMouseUp="RaktarakBtn" Background="#707070">
                                            <icon:Close Fill="#FFFFFFFF"/>
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Row="1" Background="White">
                                        <Grid.RowDefinitions >
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="55"/>
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="0" Margin="80 20">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="275"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <!--raktarSubscreen content-->
                                            <Label Grid.Row="0" Grid.Column="0" Content="Raktárnév" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="1" Grid.Column="0" Content="Hely" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="2" Grid.Column="0" Content="Telefont" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="3" Grid.Column="0" Content="Email" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Patterns:TextBox Grid.Row="0" Grid.Column="1" x:Name="raktarSS1Nev"/>
                                            <Patterns:TextBox Grid.Row="1" Grid.Column="1" x:Name="raktarSS1Hely"/>
                                            <Patterns:TextBox Grid.Row="2" Grid.Column="1" x:Name="raktarSS1Telefon"/>
                                            <Patterns:TextBox Grid.Row="3" Grid.Column="1" x:Name="raktarSS1Email"/>
                                        </Grid>
                                        <!--button-->
                                        <Grid Grid.Row="1" Width="94" Height="25" Cursor="Hand" Background="#707070" PreviewMouseUp="RaktarakBtn">
                                            <ContentControl x:Name="raktarSS1Button" Content="Mentés" Foreground="White" FontFamily="Segoe UI" FontSize="14" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>

                            <Grid x:Name="raktarSS2" Grid.Row="1" Visibility="Collapsed" Tag="Raktár SubScreen">
                                <Grid.Background>
                                    <SolidColorBrush Color="Black" Opacity="0.5"/>
                                </Grid.Background>
                                <Grid Height="110" Width="254">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Width="254" Grid.Row="0" Background="#707070" HorizontalAlignment="Right">
                                        <Label HorizontalAlignment="Left" Content="Raktár törlése" Foreground="White" FontSize="14" FontFamily="Segoe UI" Margin="5 0"/>
                                        <!--button-->
                                        <Grid Width="30" Height="30" ToolTip="Vissza" Cursor="Hand" HorizontalAlignment="Right" PreviewMouseUp="RaktarakBtn" Background="#707070">
                                            <icon:Close Fill="#FFFFFFFF"/>
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Row="1" Background="White">
                                        <Grid.RowDefinitions >
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="33"/>
                                        </Grid.RowDefinitions>
                                        <!--raktarSubscreen content-->
                                        <Label Content="Biztos törli az elemet?" Grid.Row="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="#616161" FontFamily="Segoe UI" FontSize="14"/>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <!--buttons-->
                                            <Grid Grid.Column="0" Background="#E0E0E0" Cursor="Hand" Margin="3 3 1.5 3" PreviewMouseUp="RaktarakBtn">
                                                <ContentControl Content="Igen" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#707070" FontFamily="Segoe UI" FontSize="14"/>
                                            </Grid>
                                            <Grid Grid.Column="1" Background="#E0E0E0" Cursor="Hand" Margin="1.5 3 3 3" PreviewMouseUp="RaktarakBtn">
                                                <ContentControl Content="Nem" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#707070" FontFamily="Segoe UI" FontSize="14"/>
                                            </Grid>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>

                        </Grid>
                    </TabItem>
                    <!--TERMEK LAP-->
                    <TabItem x:Name="tbiTermekek" Visibility="Visible">
                        <Grid x:Name="termek">
                            
                            <Grid x:Name="termekMS" Margin="20" Background="White" Tag="Termék MainScreen">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="40"/>
                                </Grid.RowDefinitions>
                                <!--table header-->
                                <header:Termek Grid.Row="0"/>
                                <!--table body-->
                                <ScrollViewer Grid.Row="1" HorizontalScrollBarVisibility="Disabled">
                                    <StackPanel x:Name="spTermekTabla" Orientation="Vertical"/>
                                </ScrollViewer>
                                <!--buttons-->
                                <StackPanel x:Name="spTermekButtons" Grid.Row="3" Background="#E0E0E0" Orientation="Horizontal">
                                    <Grid Width="40" Height="40" Cursor="Hand" ToolTip="Termék hozzáadása" PreviewMouseUp="TermekekBtn" Background="#E0E0E0" >
                                        <icon:Add1 Fill="#707070"/>
                                    </Grid>
                                    <Grid Width="40" Height="40" Cursor="Hand" Tag="Secret" ToolTip="Termék módosítása" Visibility="Collapsed" PreviewMouseUp="TermekekBtn" Background="#E0E0E0"  >
                                        <icon:Edit Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" Tag="Secret" ToolTip="Termék törlése" Visibility="Collapsed" PreviewMouseUp="TermekekBtn" Background="#E0E0E0" >
                                        <icon:Bin Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" ToolTip="Szinkronizálás" PreviewMouseUp="TermekekBtn" Background="#E0E0E0" >
                                        <icon:Synch Fill="#707070"/>
                                    </Grid>
                                </StackPanel>
                            </Grid>


                            <Grid x:Name="termekSS1" Grid.Row="1" Visibility="Collapsed" Tag="Termék SubScreen">
                                <Grid.Background>
                                    <SolidColorBrush Color="Black" Opacity="0.5"/>
                                </Grid.Background>
                                <Grid
                                    Height="300"
                                    Width="540">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Width="540" Grid.Row="0" Background="#707070" HorizontalAlignment="Right">
                                        <Label x:Name="termekSS1Fejlec" HorizontalAlignment="Left" Content="Termék hozzáadása" Foreground="White" FontSize="14" FontFamily="Segoe UI" Margin="5 0" />
                                        <!--button-->
                                        <Grid Width="30" Height="30" ToolTip="Vissza" Cursor="Hand" HorizontalAlignment="Right" PreviewMouseUp="TermekekBtn" Background="#707070">
                                            <icon:Close Fill="#FFFFFFFF"/>
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Row="1" Background="White">
                                        <Grid.RowDefinitions >
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="55"/>
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="0" Margin="80 20">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="235"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <!--termekSubscreen content-->
                                            <Label Grid.Row="0" Grid.Column="0" Content="Megnevezés" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="1" Grid.Column="0" Content="ÁFA" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="2" Grid.Column="0" Content="Mennyiségi Egység" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Label Grid.Row="3" Grid.Column="0" Content="Egységár" FontFamily="Segoe UI" FontSize="14" Foreground="#5D5D5D" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"/>
                                            <Patterns:TextBox Grid.Row="0" Grid.Column="1" x:Name="termekSS1Nev"/>
                                            <Patterns:TextBox Grid.Row="1" Grid.Column="1" x:Name="termekSS1AFA"/>
                                            <Patterns:TextBox Grid.Row="2" Grid.Column="1" x:Name="termekSS1ME"/>
                                            <Patterns:TextBox Grid.Row="3" Grid.Column="1" x:Name="termekSS1Ar"/>
                                        </Grid>
                                        <!--button-->
                                        <Grid Grid.Row="1" Width="94" Height="25" Cursor="Hand" Background="#707070" PreviewMouseUp="TermekekBtn">
                                            <ContentControl x:Name="termekSS1Button" Content="Mentés" Foreground="White" FontFamily="Segoe UI" FontSize="14" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>


                            <Grid x:Name="termekSS2" Visibility="Collapsed" Grid.Row="1" Tag="Termék SubScreen: Törlés">
                                <Grid.Background>
                                    <SolidColorBrush Color="Black" Opacity="0.5"/>
                                </Grid.Background>
                                <Grid Height="110" Width="254">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid Width="254" Grid.Row="0" Background="#707070" HorizontalAlignment="Right">
                                        <Label HorizontalAlignment="Left" Content="Termék törlése" Foreground="White" FontSize="14" FontFamily="Segoe UI" Margin="5 0"/>
                                        <!--button-->
                                        <Grid Width="30" Height="30" ToolTip="Vissza" Cursor="Hand" HorizontalAlignment="Right" PreviewMouseUp="TermekekBtn" Background="#707070">
                                            <icon:Close Fill="#FFFFFFFF"/>
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Row="1" Background="White">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="34"/>
                                        </Grid.RowDefinitions>
                                        <!--termekSubscreen content-->
                                        <Label Content="Biztos törli az elemet?" Grid.Row="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="#616161" FontFamily="Segoe UI" FontSize="14"/>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <!--buttons-->
                                            <Grid Grid.Column="0" Background="#E0E0E0" Cursor="Hand" Margin="3 3 1.5 3" PreviewMouseUp="TermekekBtn">
                                                <ContentControl Content="Igen" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#707070" FontFamily="Segoe UI" FontSize="14"/>
                                            </Grid>
                                            <Grid Grid.Column="1" Background="#E0E0E0" Cursor="Hand" Margin="1.5 3 3 3" PreviewMouseUp="TermekekBtn">
                                                <ContentControl Content="Nem" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#707070" FontFamily="Segoe UI" FontSize="14"/>
                                            </Grid>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>

                    </TabItem>
                    <!--ÁRUKULDES LAP-->
                    <TabItem  x:Name="tbiAtadas" Visibility="Visible">
                        <Grid x:Name="atadas">
                            <Grid Margin="20" Background="White" Name="atadasMS">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="40"/>
                                </Grid.RowDefinitions>
                                <!--table header-->
                                <header:Arukuldes Grid.Row="0"/>
