                                        <icon:Edit Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" Tag="Secret" ToolTip="Áruküldés törlése" Visibility="Collapsed" PreviewMouseUp="ArukuldesekBtn" Background="#E0E0E0" >
                                        <icon:Bin Fill="#707070"/>
                                    </Grid>
                                    <Grid Height="40" Width="40" Cursor="Hand" ToolTip="Szinkronizálás" PreviewMouseUp="ArukuldesekBtn" Background="#E0E0E0" >
                                        <icon:Synch Fill="#707070"/>
                                    </Grid>
                                </StackPanel>
                            </Grid>

