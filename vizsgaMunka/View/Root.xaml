                        </Button>
                    </Grid>
                </Label>


                <Button Width="215"
                        HorizontalAlignment="Right"
                        Padding="-1"
                        BorderThickness="0"
                        VerticalContentAlignment="Stretch"
                        HorizontalContentAlignment="Stretch"
                        Click="BtnFiokBeallitasokJobbFelsoSarok"
                        Cursor="Hand" >
                    <Grid
                        x:Name="GrFiokBeall"
                        Background="#F6F6F6">
                        <Grid
                            Background="Transparent">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="60"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <icon:ProfilPic Grid.Row="0" Fill="LightGray"/>
                            <Grid Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Label
                                    Name="lbAktualUser"
                                    Grid.Row="0"
                                    Content="Szántó Benjámin"
                                    FontFamily="Segoe UI"
                                    FontSize="13"
                                    FontWeight="Bold"
                                    Foreground="#707070"
                                    VerticalContentAlignment="Bottom"
                                    Padding="5 0"
                                    Tag="0"/>
                                <Grid Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Label Grid.Column="0"
                                           Content="@vellt"
                                           Margin="0 -4"
                                           FontSize="11"
                                           Foreground="#707070"
                                           FontFamily="Segoe UI"/>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </Button>
            </Grid>

            <!--tartalom-->
            <Grid Grid.Row="1" Background="#EBEBEB">
                <TabControl  x:Name="tbcrlTartalom" Background="Transparent" BorderThickness="0">
                    <!--AKTIVITAS LAP-->
                    <TabItem x:Name="tbiAktivitas" Visibility="Visible">
                        <ScrollViewer HorizontalScrollBarVisibility="Disabled">
                            <StackPanel Margin="0 20 0 0"  Name="ertesitesekSP">
                                <!--tartalom:Aktivitasok-->
                            </StackPanel>
                        </ScrollViewer>
                    </TabItem>
                    <!--STATISZTIKA LAP-->
                    <TabItem x:Name="tbiStatisztika" Visibility="Collapsed">

                    </TabItem>
                    
